#include "main.h"

/**
* _memcpy - copiees bytes from one memory area to another
* @dest: the pasting area
* @src: the area to be copied from
* @n: max number of bytes to be copied
* Return: a pointer to dest
*/
char *_memcpy(char *dest, char src, unsigned int n)
{
unsigned int i;

for (i = 0; i < n; i++)
{
dest[i] = src[i];
}
return (dest);
}
