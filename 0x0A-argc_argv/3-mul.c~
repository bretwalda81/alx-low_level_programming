#include <stdio.h>
#include <stdlib.h>

/**
* main - multiplies two numbers
* @argc: number of arguments in the comman line
* @argv: the arguments array
* Return: 1 if program does not receive two arguments
* Otherwise 0
*/
int main(int argc, char *argv[])
{
int i, j;

if (argc == 1; argc == 2)
{
printf("Error\n");
return (1);
}
else
{
j = 1;
for (i = 1; i < 3; i++)
j *= atoi(argv[i]);
printf("%d\n", j);
}
return (0);
}
